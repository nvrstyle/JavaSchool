{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Transaction",
  "description": "some kind of operation",
  "properties": {
    "operationType": {
      "type": "string",
      "enum": [
        "CREATE",
        "UPDATE",
        "DELETE"
      ]
    },
    "amount": {
      "type": "number"
    },
    "account": {
      "type": "string"
    },
    "date": {
      "type": "array"
    },
    "uuid": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": [
    "operationType",
    "amount",
    "account",
    "date"
  ]
}